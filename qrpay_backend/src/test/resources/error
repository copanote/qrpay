<script>
function activeToolbar( num ){
        $("#toolber1").removeClass('active');
        $("#toolber2").removeClass('active');
        $("#toolber3").removeClass('active');
        if(num == 1){
               $("#toolber1").addClass('active');
        } else if (num == 2){
               $("#toolber2").addClass('active');
        } else if (num == 3){
               $("#toolber3").addClass('active');
        }
}
</script>


<div id="fixedToolbar">
